# This file is managed by puppet. Any changes to this file will be
# overwritten the next time puppet runs. Update with caution!
[<%= @name %>]
name = <%= @repotitle %>
release = <%= @release %>
arch = <%= @arch %>
metadata = <%= if @metadata.is_a? Array; then @metadata.join(' '); else @metadata; end %>
<% if @iso -%>
iso = <%= @iso %>
<% end -%>
<% @urls.keys.sort.each do |rname| -%>
<%= rname %> = <%= @urls[rname] %>
<% end -%>
